<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    
<mapper namespace="com.ssafy.eatnote.model.dao.FoodInfoDao">
	<select id="selectByFoodCode" resultType="com.ssafy.eatnote.model.dto.FoodInfo">
        SELECT * FROM food_info
        WHERE food_code = #{foodCode}
    </select>

</mapper>
